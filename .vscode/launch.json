{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Open HTML in Browser",
            "type": "pwa-chrome",
            "request": "launch",
            "url": "http://localhost:8000/enrichment-calculator.html",
            "webRoot": "${workspaceFolder}",
            "preLaunchTask": "serve",
            "serverReadyAction": {
                "pattern": "Serving HTTP on",
                "uriFormat": "http://localhost:8000/enrichment-calculator.html"
            }
        }
    ]
}
