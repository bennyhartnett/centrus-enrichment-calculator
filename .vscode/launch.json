{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run HTTP Server",
            "type": "python",
            "request": "launch",
            "module": "http.server",
            "args": ["8000"],
            "console": "integratedTerminal",
            "python": "py",
            "serverReadyAction": {
                "pattern": "http://.*:8000/",
                "uriFormat": "http://localhost:8000/enrichment-calculator.html",
                "action": "openExternally"
            }
        }
    ]
}
